{
  "gateway": {
    "mode": "local",
    "port": 18789,
    "bind": "loopback",
    "auth": {
      "mode": "token"
    },
    "controlUi": {
      "allowInsecureAuth": true
    }
  },
  "agents": {
    "defaults": {
      "workspace": "/Users/fabrizioguespe/DevRel/openclaw-logistics/convos-concierge/workspace",
      "skipBootstrap": true,
      "model": {
        "primary": "openai/gpt-5.1-codex-mini"
      }
    }
  },
  "plugins": {
    "slots": {
      "memory": "none"
    },
    "load": {},
    "entries": {
      "convos": {
        "enabled": true
      }
    }
  },
  "skills": {
    "entries": {
      "bankr": {
        "enabled": true,
        "config": {
          "apiKey": "${BANKR_API_KEY}",
          "apiUrl": "https://api.bankr.bot"
        },
        "env": {  
          "BANKR_API_KEY": "${BANKR_API_KEY}"
        }
      },
      "agentmail": {
        "enabled": true,
        "apiKey": "${AGENTMAIL_API_KEY}",
        "env": {
          "AGENTMAIL_API_KEY": "${AGENTMAIL_API_KEY}"
        }
      }
    }
  },
  "channels": {
    "convos": {
      "XMTP_ENV": "dev"
    }
  },
  "tools": {
    "profile": "full",
    "deny": [],
    "exec": {
      "host": "gateway",
      "security": "full",
      "ask": "off"
    }
  }
}