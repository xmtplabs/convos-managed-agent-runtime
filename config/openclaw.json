{
  "gateway": {
    "mode": "local",
    "port": 18789,
    "bind": "loopback",
    "auth": {
      "mode": "token"
    },
    "controlUi": {
      "allowInsecureAuth": true
    }
  },
  "logging": {
    "consoleLevel": "debug",
    "consoleStyle": "pretty"
  },
  "browser": {
    "enabled": true,
    "remoteCdpTimeoutMs": 5000,
    "remoteCdpHandshakeTimeoutMs": 8000,
    "defaultProfile": "openclaw",
    "color": "#FF4500",
    "headless": "${HEADLESS}",
    "noSandbox": true,
    "attachOnly": false,
    "executablePath": "/Applications/Google Chrome.app/Contents/MacOS/Google Chrome",
    "profiles": {
      "openclaw": {
        "cdpPort": 18800,
        "color": "#FF4500"
      },
      "work": {
        "cdpPort": 18801,
        "color": "#0066CC"
      },
      "remote": {
        "cdpUrl": "http://10.0.0.42:9222",
        "color": "#00AA00"
      }
    }
  },
  "agents": {
    "defaults": {
      "workspace": "~/.openclaw/workspace",
      "sandbox": { "mode": "off" },
      "model": {
        "primary": "${OPENCLAW_PRIMARY_MODEL}"
      },
      "models": {
        "openrouter/openai/gpt-5.1-codex-mini": {},
        "openrouter/perplexity/sonar": {},
        "openrouter/openai/gpt-5.2-codex": {},
        "openrouter/anthropic/claude-sonnet-4": {},
        "openrouter/anthropic/claude-opus-4": {},
        "openrouter/google/gemini-2.0-flash-exp": {},
        "openrouter/meta-llama/llama-3.3-70b-instruct": {},
        "openrouter/deepseek/deepseek-r1": {},
        "openrouter/qwen/qwen-2.5-72b-instruct": {},
        "openrouter/openai/gpt-4o": {},
        "openrouter/openai/gpt-4o-mini": {},
        "openrouter/mistralai/mistral-large-2411": {}
      },
      "subagents": {
        "model": "${OPENCLAW_PRIMARY_MODEL}",
        "thinking": "medium",
        "maxConcurrent": 4,
        "archiveAfterMinutes": 30
      }
    },
    "list": [
      {
        "id": "main-agent",
        "default": true,
        "name": "Convos Agent",
        "tools": {
          "deny": [
            "web_search",
            "web_fetch"
          ]
        },
        "subagents": {
          "allowAgents": [
            "browser-automation-subagent",
            "email-subagent",
            "search-subagent"
          ]
        }
      },
      {
        "id": "search-subagent",
        "name": "Web Search",
        "tools": {
          "allow": [
            "web_search",
            "web_fetch"
          ]
        }
      },
      {
        "id": "browser-automation-subagent",
        "name": "Web Automation",
        "tools": {
          "allow": [
            "browser"
          ]
        }
      },
      {
        "id": "email-subagent",
        "name": "Email Agent"
      }
    ]
  },
  "plugins": {
    "slots": {
      "memory": "none"
    },
    "load": {},
    "entries": {
      "convos": {
        "enabled": true
      }
    }
  },
  "skills": {
    "entries": {
      "agentmail": {
        "enabled": true,
        "apiKey": "${AGENTMAIL_API_KEY}",
        "env": {
          "AGENTMAIL_API_KEY": "${AGENTMAIL_API_KEY}",
          "AGENTMAIL_INBOX_ID": "convos@agentmail.to"
        }
      }
    }
  },
  "channels": {
    "convos": {
      "XMTP_ENV": "dev",
      "enabled": true
    }
  },
  "tools": {
    "deny": [],
    "exec": {
      "host": "gateway",
      "security": "full",
      "ask": "off"
    },
    "web": {
      "search": {
        "apiKey": "${OPENROUTER_API_KEY}",
        "provider": "perplexity",
        "maxResults": 5,
        "timeoutSeconds": 30,
        "perplexity": {
          "apiKey": "${OPENROUTER_API_KEY}",
          "baseUrl": "https://openrouter.ai/api/v1",
          "model": "perplexity/sonar"
        }
      }
    }
  },
  "models": {
    "mode": "merge",
    "providers": {
      "openrouter": {
        "baseUrl": "https://openrouter.ai/api/v1",
        "apiKey": "${OPENROUTER_API_KEY}",
        "api": "openai-completions",
        "models": [
          {
            "id": "openrouter/openai/gpt-5.1-codex-mini",
            "name": "GPT-5.1 Codex Mini",
            "reasoning": false,
            "input": [
              "text"
            ],
            "cost": {
              "input": 0,
              "output": 0,
              "cacheRead": 0,
              "cacheWrite": 0
            },
            "contextWindow": 200000
          },
          {
