{
  "gateway": {
    "mode": "local",
    "port": 18789,
    "bind": "loopback",
    "auth": {
      "mode": "token"
    },
    "controlUi": {
      "allowInsecureAuth": true
    }
  },
  "agents": {
    "defaults": {
      "skipBootstrap": true,
      "model": {
        "primary": "openai/gpt-5.1-codex-mini"
      }
    }
  },
  "plugins": {
    "slots": {
      "memory": "none"
    }
  },
  "skills": {
    "entries": {
      "bankr": {
        "apiKey": "",
        "config": { "apiUrl": "https://api.bankr.bot" }
      },
      "agentmail": {
        "apiKey": ""
      }
    }
  },
  "tools": {
    "allow": [],
    "deny": []
  }
}
