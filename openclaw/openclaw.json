{
  "gateway": {
    "mode": "local",
    "port": 18789,
    "bind": "loopback",
    "trustedProxies": ["::1", "127.0.0.1"],
    "auth": {
      "mode": "token"
    },
    "controlUi": {
      "allowInsecureAuth": true,
      "dangerouslyDisableDeviceAuth": true
    }
  },
  "logging": {
    "consoleLevel": "debug",
    "consoleStyle": "pretty"
  },

  "commands": {
    "native": "auto",
    "nativeSkills": "auto",
    "restart": true
  },
  "browser": {
    "enabled": true,
    "remoteCdpTimeoutMs": 5000,
    "remoteCdpHandshakeTimeoutMs": 8000,
    "defaultProfile": "openclaw",
    "color": "#FF4500",
    "headless": true,
    "noSandbox": true,
    "attachOnly": false,
    "executablePath": "/Applications/Google Chrome.app/Contents/MacOS/Google Chrome",
    "profiles": {
      "openclaw": {
        "cdpPort": 18800,
        "color": "#FF4500"
      }
    }
  },
  "agents": {
    "defaults": {
      "workspace": "~/.openclaw/workspace",
      "sandbox": { "mode": "off" },
      "blockStreamingDefault": "on",
    
      "model": {
        "primary": "${OPENCLAW_PRIMARY_MODEL}"
      },
      "models": {
        "openrouter/openai/gpt-oss-20b": {},
        "openrouter/openai/gpt-5.1-codex-mini": {},
        "openrouter/perplexity/sonar": {},
        "openrouter/openai/gpt-5.2-codex": {},
        "openrouter/anthropic/claude-sonnet-4-6": {},
        "openrouter/anthropic/claude-sonnet-4.6": {},
        "openrouter/anthropic/claude-opus-4-6": {},
        "openrouter/anthropic/claude-opus-4.6": {},
        "openrouter/google/gemini-2.0-flash-exp": {},
        "openrouter/meta-llama/llama-3.3-70b-instruct": {},
        "openrouter/deepseek/deepseek-r1": {},
        "openrouter/qwen/qwen-2.5-72b-instruct": {},
        "openrouter/openai/gpt-4o": {},
        "openrouter/openai/gpt-4o-mini": {},
        "openrouter/mistralai/mistral-large-2411": {}
      }
    }
  },
  "skills": {
    "entries": {
      "agentmail": {
        "enabled": true,
        "apiKey": "${AGENTMAIL_API_KEY}",
        "env": {
          "AGENTMAIL_API_KEY": "${AGENTMAIL_API_KEY}",
          "AGENTMAIL_INBOX_ID": "${AGENTMAIL_INBOX_ID}"
        }
      },
      "bankr": {
        "enabled": true,
        "apiKey": "${BANKR_API_KEY}",
        "env": {
          "BANKR_API_KEY": "${BANKR_API_KEY}"
        }
      },
      "telnyx-cli": {
        "enabled": true,
        "apiKey": "${TELNYX_API_KEY}",
        "env": {
          "TELNYX_API_KEY": "${TELNYX_API_KEY}",
          "TELNYX_PHONE_NUMBER": "${TELNYX_PHONE_NUMBER}",
          "TELNYX_MESSAGING_PROFILE_ID": "${TELNYX_MESSAGING_PROFILE_ID}"
        }
      }
    }
  },
  "channels": {
    "convos": {
      "debug": true,
      "enabled": true,
      "env": "${XMTP_ENV}"
    }
  },
  "tools": {
    "deny": [],
    "exec": {
      "host": "gateway",
      "security": "full",
      "ask": "off"
    },
    "web": {
      "search": {
        "apiKey": "${OPENROUTER_API_KEY}",
        "provider": "perplexity",
        "maxResults": 5,
        "timeoutSeconds": 30,
        "perplexity": {
          "apiKey": "${OPENROUTER_API_KEY}",
          "baseUrl": "https://openrouter.ai/api/v1",
          "model": "perplexity/sonar"
        }
      }
    }
  },
  "plugins": {
    "allow": ["convos", "web-tools"],
    "slots": { "memory": "none" },
    "entries": { "convos": { "enabled": true }, "web-tools": { "enabled": true } }
  },
  "models": {
    "mode": "merge",
    "providers": {
      "openrouter": {
        "baseUrl": "https://openrouter.ai/api/v1",
        "apiKey": "${OPENROUTER_API_KEY}",
        "api": "openai-completions",
        "models": [
          { "id": "openrouter/openai/gpt-oss-20b", "name": "GPT-OSS 20B" },
          { "id": "openrouter/openai/gpt-5.1-codex-mini", "name": "GPT-5.1 Codex Mini", "input": ["text", "image"] },
          { "id": "openrouter/perplexity/sonar", "name": "Perplexity Sonar" },
          { "id": "openrouter/openai/gpt-5.2-codex", "name": "GPT-5.2 Codex", "input": ["text", "image"] },
          { "id": "openrouter/anthropic/claude-sonnet-4-6", "name": "Claude Sonnet 4", "input": ["text", "image"] },
          { "id": "openrouter/anthropic/claude-sonnet-4.6", "name": "Claude Sonnet 4.6", "input": ["text", "image"] },
          { "id": "openrouter/anthropic/claude-opus-4-6", "name": "Claude Opus 4", "input": ["text", "image"] },
          { "id": "openrouter/anthropic/claude-opus-4.6", "name": "Claude Opus 4.6", "input": ["text", "image"] },
          { "id": "openrouter/google/gemini-2.0-flash-exp", "name": "Gemini 2.0 Flash", "input": ["text", "image"] },
          { "id": "openrouter/meta-llama/llama-3.3-70b-instruct", "name": "Llama 3.3 70B" },
          { "id": "openrouter/deepseek/deepseek-r1", "name": "DeepSeek R1" },
          { "id": "openrouter/qwen/qwen-2.5-72b-instruct", "name": "Qwen 2.5 72B" },
          { "id": "openrouter/openai/gpt-4o", "name": "GPT-4o", "input": ["text", "image"] },
          { "id": "openrouter/openai/gpt-4o-mini", "name": "GPT-4o Mini", "input": ["text", "image"] },
          { "id": "openrouter/mistralai/mistral-large-2411", "name": "Mistral Large" },

          { "id": "anthropic/claude-sonnet-4-6", "name": "Claude Sonnet 4", "input": ["text", "image"] },
          { "id": "anthropic/claude-sonnet-4.6", "name": "Claude Sonnet 4.6", "input": ["text", "image"] },
          { "id": "anthropic/claude-opus-4-6", "name": "Claude Opus 4", "input": ["text", "image"] },
          { "id": "anthropic/claude-opus-4.6", "name": "Claude Opus 4.6", "input": ["text", "image"] },
          { "id": "openai/gpt-5.1-codex-mini", "name": "GPT-5.1 Codex Mini", "input": ["text", "image"] },
          { "id": "openai/gpt-5.2-codex", "name": "GPT-5.2 Codex", "input": ["text", "image"] },
          { "id": "google/gemini-2.0-flash-exp", "name": "Gemini 2.0 Flash", "input": ["text", "image"] },
          { "id": "openai/gpt-4o", "name": "GPT-4o", "input": ["text", "image"] },
          { "id": "openai/gpt-4o-mini", "name": "GPT-4o Mini", "input": ["text", "image"] }
        ]
      }
    }
  }
}
