# Pool Manager Config
PORT=3001                                      # Railway overrides this to 8080
POOL_API_KEY=your-shared-pool-secret          # different per environment

# Railway — agent instances deploy from the same repo/branch as the pool manager (auto-detected)
# RAILWAY_SOURCE_REPO=                          # override: defaults to RAILWAY_GIT_REPO_OWNER/RAILWAY_GIT_REPO_NAME
# RAILWAY_SOURCE_BRANCH=                        # override: defaults to RAILWAY_GIT_BRANCH
RAILWAY_PROJECT_ID=your-project-id
RAILWAY_ENVIRONMENT_ID=your-environment-id
RAILWAY_API_TOKEN=your-railway-token

# Env vars injected into each agent instance
OPENCLAW_PRIMARY_MODEL=openrouter/openai/gpt-oss-20b
XMTP_ENV=dev                                  # "dev" for staging, "production" for production
AGENTMAIL_API_KEY=                             # per-instance inboxes created/deleted automatically
AGENTMAIL_DOMAIN=                              # custom domain for inboxes (e.g. mail.convos.org); defaults to agentmail.to
BANKR_API_KEY=
TELNYX_API_KEY=
TELNYX_PHONE_NUMBER=
TELNYX_MESSAGING_PROFILE_ID=

# OpenRouter — per-instance keys created/deleted automatically
OPENROUTER_MANAGEMENT_KEY=                     # sk-or-v1-... management key for creating per-instance API keys
OPENROUTER_KEY_LIMIT=20                        # USD spend limit per key
OPENROUTER_KEY_LIMIT_RESET=monthly

# Pool sizing
POOL_MIN_IDLE=1

# Postgres (Railway internal URL)
DATABASE_URL=postgresql://...
